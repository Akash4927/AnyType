// Join{{.}}Slice sends inputs on the given out channel and returns a done channel to receive one signal when inp has been drained
func Join{{.}}Slice(out chan<- {{.Dot}}, inp ...[]{{.Dot}}) chan struct{} {
	done := make(chan struct{})
	go func() {
		defer close(done)
		for _, in := range inp {
			for _, i := range in {
				out <- i
			}
		}
		done <- struct{}{}
	}()
	return done
}
